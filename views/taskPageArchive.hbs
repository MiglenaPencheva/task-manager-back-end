<main>

    <h1 class="text-center">Завършени задачи</h1>

    {{#if completed}}

    <h4 class="text-center">{{completed.length}} завършени задачи</h4>

    {{!-- {{> search}} --}}

    <div class="container mt-3">
        <div class="row ">
            <div class="card-deck d-flex justify-content-center">

                {{#each completed}}
                {{> task}}
                {{/each}}

            </div>
        </div>
    </div>

    {{else}}
    <h3 class="text-center">Няма завършени задачи...</h3>
    {{/if}}

</main>